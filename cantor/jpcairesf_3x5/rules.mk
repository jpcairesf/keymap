# This file should be placed in your keymap directory.
# (e.g., qmk_firmware/keyboards/cantor/keymaps/your_keymap_name/rules.mk)

# Enable combos for features like J+K for Escape.
COMBO_ENABLE = yes

# Enable "extra" keys for media and brightness controls (Volume, Play/Pause, etc.).
EXTRAKEY_ENABLE = yes

# Enable N-key rollover for better performance with multiple simultaneous key presses.
NKRO_ENABLE = yes
